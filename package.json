{
  "name": "@borgar/ordavinda",
  "type": "module",
  "version": "2.0.0",
  "description": "Orðavinda is a simple JavaScript word game. Given a set of 6 letters, the player must use them to find as many valid words as possible before time runs out. In order to qualify to play the next round, the player must guess at least one of the 6 letter word combinations.",
  "main": "index.js",
  "scripts": {
    "filterbin": "node scripts/a_filter_bin.mjs",
    "levelgen": "node scripts/b_build_levels.mjs",
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development --hot --history-api-fallback",
    "lint": "eslint --ext .js,.jsx src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Borgar Þorsteinsson <borgar@borgar.net> (http://borgar.net/)",
  "license": "MIT",
  "dependencies": {
