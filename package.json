    "icelandic": "~1.3.0",
    "react": "~18.3.1",
    "react-dom": "~18.3.1"
  },
  "devDependencies": {
    "@babel/core": "~7.26.0",
    "@babel/eslint-parser": "~7.25.9",
    "@babel/preset-env": "~7.26.0",
    "@babel/preset-react": "~7.25.9",
    "@borgar/eslint-config": "~3.1.0",
    "@types/react": "~18.3.12",
    "babel-loader": "~9.2.1",
    "copy-webpack-plugin": "~12.0.2",
