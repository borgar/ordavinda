    "prop-types": "~15.8.1",
    "style-loader": "~4.0.0",
    "webpack": "~5.96.1",
    "webpack-cli": "~5.1.4",
    "webpack-dev-server": "~5.1.0",
    "webpack-merge": "~6.0.1"
  }
}
